{
  "name": "@ailytic/archen",
  "version": "0.0.0-local-dev.0",
  "description": "Archen - Add GraphQL API to your existing applications",
  "main": "./dist/index.js",
  "files": [
    "bin",
    "dist",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "author": "Ailytic",
  "license": "MIT",
  "dependencies": {
    "@ailytic/sqlit": "^2.1.0-alpha.5",
    "dataloader": "^1.4.0",
    "graphql": "^0.13.2"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/fangwd/archen.git"
  },
  "devDependencies": {
    "@types/graphql": "^0.12.5",
    "@types/jest": "^22.2.0",
    "@types/mysql": "^2.15.4",
    "@types/node": "^9.4.7",
    "jest": "^22.4.1",
    "mysql": "^2.16.0",
    "ts-jest": "^22.4.1",
    "typescript": "3.4.5"
  },
  "scripts": {
    "test": "DB_TYPE=mysql jest",
    "build": "tsc",
    "exec": "exec"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "/test/.+\\.test.ts",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "verbose": true
  }
}
